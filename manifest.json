{
  "manifest_version": 3,
  "name": "Color Blindness Filter",
  "version": "1.0",
  "description": "Filters colors on websites for color-blind users.",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      
    
    }
  },
  "icons": {
    
   
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
